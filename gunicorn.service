[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ec2-user
WorkingDirectory=/var/lib/jenkins/workspace/pipeline
ExecStart=/var/lib/jenkins/workspace/pipeline/env/bin/gunicorn --workers 3 --log-level debug --error-logfile /var/lib/jenkins/workspace/pipeline/error.log --bind unix:/var/lib/jenkins/workspace/pipeline/app.sock DjangoApp.wsgi:application

[Install]
WantedBy=multi-user.target
