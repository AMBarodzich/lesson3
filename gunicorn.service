[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
WorkingDirectory=/var/lib/jenkins/workspace/PythonApplication
ExecStart=/var/lib/jenkins/workspace/PythonApplication/env/bin/gunicorn --workers 3 --log-level debug --error-logfile /var/lib/jenkins/workspace/PythonApplication/error.log --bind unix:/var/lib/jenkins/workspace/PythonApplication/app.sock DjangoApp.wsgi:application

[Install]
WantedBy=multi-user.target
